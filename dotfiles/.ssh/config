Host github.com
  HostName github.com
  IdentityFile ~/.ssh/id_rsa
  IdentitiesOnly yes

Host lxrm
  HostName lxrm
  User janjoh

Host jth-jump-host
  HostName lxrm.hj.se
  User janjoh
  IdentityFile /home/johan/.ssh/id_rsa

Host jth-comphub
  HostName jth-comphub.hj.se
  User janjoh
  IdentityFile /home/janjoh/.ssh/id_ed25519
  ProxyCommand ssh -W %h:%p jth-jump-host
  Port 22

Host jth-autograde
  HostName jth-autograde.hj.se
  User janjoh
  IdentityFile /home/janjoh/.ssh/id_ed25519
  ProxyCommand ssh -W %h:%p jth-jump-host
  Port 22
